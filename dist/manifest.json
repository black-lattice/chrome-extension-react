{
    "manifest_version": 3,
    "name": "收藏主页",
    "version": "1.0",
    "description": "一个功能丰富的新标签页替代方案",
    "icons": {
      "16": "icons/logo16.png",
      "48": "icons/logo48.png", 
      "128": "icons/logo128.png"
    },
    "permissions": [
      "tabs",
      "bookmarks",
      "history",
      "storage",
      "webRequest",
      "chrome://favicon/",
      "favicon"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "chrome_url_overrides": {
      "newtab": "index.html"
    }
  }
  